{
  "title": "Update user",
  "description": "Update a user object",
  "type":"object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 2
    },
    "surname": {
      "type": "string",
      "minLength": 3
    },
    "email": {
      "type": "string",
      "format": "email",
      "minLength": 3
    },
    "phone": {
      "type": "string"
    },
    "image_file": {
      "type": "string",
      "minLength": 1
    },
    "is_active": {
      "type": "boolean"
    }
  }
}
