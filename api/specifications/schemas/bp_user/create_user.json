{
  "title": "Create user",
  "description": "Create a user object",
  "type":"object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "required": [
    "email",
    "password"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email",
      "minLength": 5
    },
    "password": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    }
  }
}
